{
  "name": "shop-vite",
  "version": "10.0.5",
  "private": true,
  "author": "github.com/zxwk1998",
  "scripts": {
    "dev": "cross-env VITE_CJS_IGNORE_WARNING=true vite",
    "dev:vue-tsc": "vue-tsc --noEmit && cross-env VITE_CJS_IGNORE_WARNING=true vite",
    "dev:https": "cross-env VITE_CJS_IGNORE_WARNING=true vite --config ./vite.config.dev.ts",
    "vue-tsc": "vue-tsc --noEmit",
    "build": "cross-env VITE_CJS_IGNORE_WARNING=true vite build",
    "build:fast": "cross-env VITE_CJS_IGNORE_WARNING=true NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:vue-tsc": "vue-tsc --noEmit && cross-env VITE_CJS_IGNORE_WARNING=true vite build",
    "preview": "cross-env VITE_CJS_IGNORE_WARNING=true vite preview",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier {src,mock,library,types}/**/*.{html,vue,css,sass,scss,js,ts} --write",
    "lint:stylelint": "stylelint {src,mock,library}/**/*.{vue,css,sass,scss} --fix --cache --cache-location node_modules/.cache/stylelint/",
    "global:install": "npm i -g nrm cnpm npm-check-updates pnpm",
    "globle:update": "ncu -g",
    "module:install": "pnpm i",
    "module:update": "ncu -u --reject vite-plugin-mock,eslint,sass,vue-echarts && npm run module:install",
    "module:reinstall": "rimraf node_modules && npm run module:install",
    "template": "plop"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@opentiny/vue": "^3.18.0",
    "@vueuse/core": "^11.0.3",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "disable-devtool": "^0.3.7",
    "element-plus": "2.8.4",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "sortablejs": "^1.15.3",
    "typeit": "^8.8.4",
    "vsv-context-menu": "^1.4.2",
    "vsv-icon": "^1.2.2",
    "vue": "^3.4.38",
    "vue-draggable-plus": "^0.5.3",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@element-plus/eslint-config": "^2.8.1",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/compiler-sfc": "^3.4.38",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vue": "^9.28.0",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.0",
    "plop": "^4.0.1",
    "postcss": "^8.4.45",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.77.6",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.3",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-unplugin": "^1.8.0",
    "vite-plugin-vitebar": "^0.0.8",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^2.1.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://vuejs-core.cn/shop-vite",
  "license": "Mozilla Public License Version 2.0",
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint",
      "npm run lint:prettier",
      "git add"
    ]
  },
  "participants": [
    "FlowPeakFish"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxwk2024/shop-vite.git"
  }
}
